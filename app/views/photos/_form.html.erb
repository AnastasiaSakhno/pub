<%= form_for(@photo) do |f| %>
    <div><%= f.label :url %>
      <br/>
      <%= f.file_field :url %>
      <%= f.hidden_field :url_cache %></div>
    <div class="field">
      <%= f.label :title %>
      <br/>
      <%= f.text_field :title %>
    </div>
    <div class="field">
      <%= f.label :description %>
      <br/>
      <%= f.text_area :description %>
    </div>
    <div class="actions">
      <%= f.submit "#{t :continue}" %>
    </div>
<% end %>
